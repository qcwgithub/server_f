{
	"purpose": "test",
	"logDir": "{current}/Logs",
	"allowChannels": [
		"uuid"
	],
	"socketSecurityConfig": {
		"maxPacketSize": 65536,
		"maxRecvQueueCount": 100,
		"maxSendQueueCount": 200,
		"maxSendQueueBytes": 524288
	},
	"roomMessageConfig": {
		"recentMessagesCount": 20,
		"maxMessagesCount": 5000,
		"getHistoryMessageCount": 20,
		"minLength": 1,
		"maxLength": 200,
		"minIntervalMs": 500,
		"periodMs": 60000,
		"periodMaxCount": 60
	},
	"userNameConfig": {
		"minIntervalS": 1,
		"minLength": 2,
		"maxLength": 20
	},
	"userAvatarConfig": {
		"minIntervalS": 1,
		"minIndex": 0,
		"maxIndex": 19
	},
	"redisConfig": {
		"redisConn": "localhost",
		"db": 1,
		"requireMinVersion": "3.0"
	},
	"mongoDBConfig": {
		"mongoDBConn": "mongodb://localhost:23456",
		"dbData": "{purpose}_user_data"
	}
}